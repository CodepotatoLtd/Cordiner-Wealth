    "cross-env": "^5.1",
    "laravel-mix": "^5.0.0",
    "lodash": "^4.17.15",
    "regenerator-runtime": "^0.13.3",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
